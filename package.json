{
  "name": "vue-model-x",
  "version": "0.4.0",
  "description": "MobX-like state management library for Vue.js",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "babel --config-file ./.babelrc --delete-dir-on-start -d lib -- src",
    "build:esm": "babel --config-file ./.babelrc-esm --delete-dir-on-start -d esm -- src",
    "prepublishOnly": "npm run build && npm test",
    "test": "jest"
  },
  "keywords": [
    "vue",
    "model",
    "mobx",
    "reactive",
    "state",
    "management",
    "lib"
  ],
  "author": "th0r",
  "license": "ISC",
  "peerDependencies": {
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.44",
    "@babel/core": "7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.44",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@vue/test-utils": "1.0.0-beta.13",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.3",
    "eslint": "4.19.1",
    "eslint-config-th0r": "1.0.1",
    "jest": "22.4.3",
    "vue": "^2.5.16",
    "vue-template-compiler": "2.5.16"
  },
  "files": [
    "src/*.*",
    "lib/*.*",
    "esm/*.*"
  ]
}
